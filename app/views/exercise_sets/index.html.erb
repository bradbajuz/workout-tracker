<div class="col-sm-12">
  <% @exercise_sets.each do |e| %>
    <div class="card shadow-sm mb-3 text-center">
      <div class="card-body">
        <div class="row">
          <div class="col-lg-12">
            <h5><%= e.exercise.name %></h5>
            <p><%= e.date %></p>
            <p><%= e.workout.name %></p>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
