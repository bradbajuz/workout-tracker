<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-header">
        New Workout
      </div>
      <div class="card-body">
        <%= render partial: 'form', locals: {workout: @workout} %>
      </div>
      <div class="card-footer">
        <div class="float-right">
          <button type="submit" class="btn btn-sm btn-primary" aria-hidden="true" form="workout_form">Save</button>
        </div>
      </div>
    </div>
  </div>
</div>
